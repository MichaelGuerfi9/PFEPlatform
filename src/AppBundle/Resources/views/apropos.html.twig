            font-weight:  bold;
        }

        .trapezePurple {
            height: 450px;
            background-color: #474668;
        }

        .trapezeWhite {
            background-color: white;
            height: 300px;
        }

        .trapezePurple, .trapezeWhite {
            width: 100%;
            position: relative;
        }

        .triangleBottomR, .triangleTopR {
            border-left: 100vw solid transparent;
        }

        .triangleBottomR, .triangleTopR {
            width: 0;
            height: 0;
            position: absolute;
        }

        .triangleBottomR {
            border-bottom: 75px solid white;
            bottom: 0;
        }

        .triangleTopR {
            border-top: 75px solid white;
            top: 0;
        }

        .trapezePurple div, .trapezeWhite div {
            display: inline-block;
            vertical-align: middle;
            /*width: 100%;*/
        }

        .trapezePurple:nth-of-type(1) .leftImg, .trapezePurple:nth-of-type(1) .rightImg, .trapezePurple:nth-of-type(1) .leftText, .trapezePurple:nth-of-type(1) .rightText {
            height: calc(100% - 50px) !important;
        }

        .leftImg, .rightImg, .leftText, .rightText {
            height: 100%;
            width: 50%;
            top: 0;
            display: inline-block;
        }
        .leftImg, .rightImg{
            text-align: center;
        }
        .rightText, .leftText{
            padding: 75px 0;
        }
        .rightText{
            padding-right: 20px;
        }
        .leftText{
            padding-left: 20px;
        }
        .trapezePurple:nth-of-type(1) .rightText{
            padding: 0 0 75px 0;
        }
        .rightText div, .leftText div{
            height: 100%;
            position:  relative;
            width:  100%;
        }
        .trapeze h3{
            font-weight: bold;
            margin-bottom: 10px;
        }
        .pictoSVG{
            fill: #474668;
            background-color: white;
            border-radius: 50%;
            height: 50%;
            top: calc(25% - 37.5px);
            position: relative;
        }
        .trapezePurple .pictoSVG.resize{
            height: calc(50% - 75px);
        }
