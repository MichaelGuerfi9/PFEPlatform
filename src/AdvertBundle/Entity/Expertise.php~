    private $voyants_allumes;


    /**
     * @var string
     *
     * @ORM\Column(name="nom_voyants_allumes", type="string", length=255)
     */

    private $nom_voyants_allumes;
    //Si oui, lesquels ?


    /**
     * @var string
     *
     * @ORM\Column(name="commentaire_test_routier", type="string", length=255)
     */

    private $commentaire_test_routier;


    /**
     * @var string
     *
     * @ORM\Column(name="notation_vendeur", type="string", length=255)
     */

    private $notation_vendeur;

    /**
     * @var string
     *
     * @ORM\Column(name="achat_securise", type="string", length=255)
     */

    private $achat_securise;
    //L'achat semble sécurisé, je le recommande


    /**
     * @var string
     *
     * @ORM\Column(name="kilometrage_coherent", type="string", length=255)
     */

    private $kilometrage_coherent;
    //Kilométrage cohérent (entretien, usure...)



    /**
     * @var string
     *
     * @ORM\Column(name="commentaire_conclusion", type="string", length=255)
     */

    private $commentaire_conclusion;

    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set advert
     *
     * @param \AdvertBundle\Entity\Advert $advert
     *
     * @return Expertise
     */
    public function setAdvert(\AdvertBundle\Entity\Advert $advert = null)
    {
        $this->advert = $advert;

        return $this;
    }

    /**
     * Get advert
     *
     * @return \AdvertBundle\Entity\Advert
     */
    public function getAdvert()
    {
        return $this->advert;
    }

    /**
     * Set status
     *
     * @param string $status
     *
     * @return Expertise
     */
    public function setStatus($status)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return string
     */
    public function getStatus()
    {
        return $this->status;
    }
}
